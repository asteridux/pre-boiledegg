{
  "name": "pre-boiledegg",
  "version": "1.1.0",
  "description": "A Preact project starter",
  "main": "./build/lib.js",
  "private": true,
  "scripts": {
    "release:init": "standard-version --first-release",
    "release": "standard-version",
    "build": "webpack",
    "watch:build": "webpack --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asteridux/pre-boiledegg.git"
  },
  "author": "Antonin Januska <antonin@antjanus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asteridux/pre-boiledegg/issues"
  },
  "homepage": "https://github.com/asteridux/pre-boiledegg#readme",
  "dependencies": {
    "preact": "8.2.5"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "21.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.0",
    "chai": "4.1.2",
    "copy-webpack-plugin": "4.0.1",
    "jest": "21.0.2",
    "preact-jsx-chai": "2.2.1",
    "rimraf": "2.6.1",
    "standard-version": "4.2.0",
    "webpack": "3.5.6"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "moduleFileExtensions": ["js", "jsx"],
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "testMatch": ["**/?(*.)(spec|test).js?(x)"],
    "verbose": true
  }
}
